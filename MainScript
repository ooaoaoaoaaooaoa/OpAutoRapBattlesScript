local function EBFCRDU_fake_script() -- Scene.LocalScript 
	local script = Instance.new('LocalScript', Scene)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-77, 65, -214)
	end)
end
coroutine.wrap(EBFCRDU_fake_script)()
