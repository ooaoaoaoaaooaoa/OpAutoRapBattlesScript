-- Scripts:

local function HUDCGM_fake_script() -- Player1.LocalScript 
	local script = Instance.new('LocalScript', Player1)

	script.Parent.MouseButton1Click:Connect(function()
		for i = 0,1 do
			game.Workspace.Votes:FireServer(false,"p1")
			game:GetService("RunService").Heartbeat:Wait()
		end
	end)
end
coroutine.wrap(HUDCGM_fake_script)()
local function LCAFDW_fake_script() -- Player1.Name 
	local script = Instance.new('LocalScript', Player1)

	while true do
		script.Parent.Text = game.Workspace.RapBattles.Rappers.player1.Value
		wait(0.1)
	end
	
end
coroutine.wrap(LCAFDW_fake_script)()
local function YQWAP_fake_script() -- Player2.LocalScript 
	local script = Instance.new('LocalScript', Player2)

	script.Parent.MouseButton1Click:Connect(function()
		for i = 0,1 do
			game.Workspace.Votes:FireServer(false,"p2")
			game:GetService("RunService").Heartbeat:Wait()
		end
	end)
end
coroutine.wrap(YQWAP_fake_script)()
